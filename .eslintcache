 {\n          id: 1,\n          name : 'Умные часы Smart Pink Forever',\n          price: 1400,\n          imgSrc:'https://user-images.githubusercontent.com/61156194/104780421-74676000-5789-11eb-894e-c31e46808bd0.png',\n          quantity: 1\n        },\n        {\n          id: 2,\n          name : 'Графический планшет Wacom',\n          price: 2100,\n          imgSrc:'https://user-images.githubusercontent.com/61156194/104780467-86490300-5789-11eb-8f71-92165f88d157.jpg',\n          quantity: 1\n        }\n      ]; \n      \n      constructor (dataCards) {\n        makeAutoObservable(this, {\n          dataCards: observable, \n          getTotalSum : computed\n        })\n      }\n      get getTotalSum () {\n        return this.dataCards.reduce(((accumulator, currentValue) => accumulator + (currentValue.price * currentValue.quantity)),0);\n      }\n      incrmentQuantity (id) {\n        this.dataCards[`${id}`].quantity = ++this.dataCards[`${id}`].quantity;\n      }\n      decrementQuantity(id) {\n        if ((this.dataCards[`${id}`].quantity) > 1) {\n        this.dataCards[`${id}`].quantity = --this.dataCards[`${id}`].quantity;\n        }\n      }\n}\n\nexport default new Store();",{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":1,"column":16,"nodeType":"103","messageId":"104","endLine":1,"endColumn":25},{"ruleId":"101","severity":1,"message":"102","line":1,"column":16,"nodeType":"103","messageId":"104","endLine":1,"endColumn":25},{"ruleId":"101","severity":1,"message":"102","line":1,"column":16,"nodeType":"103","messageId":"104","endLine":1,"endColumn":25},{"ruleId":"101","severity":1,"message":"105","line":1,"column":31,"nodeType":"103","messageId":"104","endLine":1,"endColumn":37},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"no-unused-vars","'Component' is defined but never used.","Identifier","unusedVar","'action' is defined but never used.","no-global-assign","no-unsafe-negati